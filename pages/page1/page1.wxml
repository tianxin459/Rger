<!--logs.wxml-->
<text>Test</text>
<view class="listContainer">
  <view class="item {{p.gender==1?'m':'f'}} " wx:for="{{nameList}}" wx:for-item="p" wx:key="*this" hover-class="item-hover">
    <image class="avatar" src="{{p.avatarUrl}}" background-size="cover"></image>
    <text>{{p.name}}</text>
  </view>
</view>
<view class="classname">
  <button bindtap="Sort">Click</button>
</view>